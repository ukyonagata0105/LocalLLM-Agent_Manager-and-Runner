{
    "compilerOptions": {
        "target": "ES2022",
        "module": "ES2022",
        "moduleResolution": "bundler",
        "esModuleInterop": true,
        "allowSyntheticDefaultImports": true,
        "strict": true,
        "skipLibCheck": true,
        "resolveJsonModule": true,
        "outDir": "./",
        "noEmit": false,
        "declaration": false
    },
    "include": [
        "*.ts"
    ],
    "exclude": [
        "node_modules"
    ]
}