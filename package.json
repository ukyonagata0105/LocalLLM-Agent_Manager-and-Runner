{
  "name": "localllm-agent-manager",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "main": "electron/main.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "electron:build": "cd electron && tsc",
    "electron:start": "npm run electron:build && electron .",
    "electron:dev": "concurrently \"npm run dev\" \"npm run electron:start\"",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:electron": "playwright test --config=playwright.electron.config.ts",
    "package": "npm run build && npm run electron:build && electron-builder",
    "package:mac": "npm run build && npm run electron:build && electron-builder --mac",
    "package:win": "npm run build && npm run electron:build && electron-builder --win",
    "package:linux": "npm run build && npm run electron:build && electron-builder --linux"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.2",
    "@electron/get": "^4.0.2",
    "@google/generative-ai": "^0.24.1",
    "@radix-ui/react-slot": "^1.0.2",
    "@types/better-sqlite3": "^7.6.13",
    "@types/cookie-parser": "^1.4.10",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@xyflow/react": "^12.10.0",
    "better-sqlite3": "^12.5.0",
    "clsx": "^2.1.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "electron-store": "^11.0.2",
    "express": "^5.2.1",
    "express-rate-limit": "^8.2.1",
    "framer-motion": "^11.0.8",
    "i18next": "^25.7.4",
    "lucide-react": "^0.344.0",
    "node-fetch": "^2.7.0",
    "openai": "^6.15.0",
    "qrcode": "^1.5.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-grid-layout": "^1.4.4",
    "react-i18next": "^16.5.1",
    "socket.io": "^4.8.3",
    "socket.io-client": "^4.8.3",
    "speakeasy": "^2.0.0",
    "tailwind-merge": "^2.2.1",
    "yaml": "^2.8.2",
    "zustand": "^4.5.7"
  },
  "devDependencies": {
    "@electron/rebuild": "^3.7.1",
    "@playwright/test": "^1.57.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/node-fetch": "^2.6.13",
    "@types/qrcode": "^1.5.6",
    "@types/react": "^18.2.64",
    "@types/react-dom": "^18.3.7",
    "@types/react-grid-layout": "^1.3.5",
    "@types/speakeasy": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "^4.7.0",
    "autoprefixer": "^10.4.18",
    "bcryptjs": "^3.0.3",
    "concurrently": "^9.2.1",
    "electron": "^39.2.7",
    "electron-builder": "^25.1.8",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "jsdom": "^27.4.0",
    "postcss": "^8.4.35",
